{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "pod-test",
    "namespace": "test",
    "labels": {
      "app.kubernetes.io/name": "pod-test",
      "app.kubernetes.io/component": "minkapi"
    }
  },
  "spec": {
    "terminationGracePeriodSeconds": 0,
    "schedulerName": "default-scheduler",
    "preemptionPolicy": "PreemptLowerPriority",
    "containers": [
      {
        "name": "pause",
        "image": "registry.k8s.io/pause:3.5",
        "resources": {
          "requests": {
            "cpu": "100m",
            "memory": "4Gi"
          }
        }
      }
    ]
  }
}
